<?xml version="1.0"?>
<!--
  有关如何配置 ASP.NET 应用程序的详细消息，请访问
  http://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>
  <appSettings>
    <add key="AjaxNamespace" value="Shoy.Demo"/>
    <add key="FilePath" value="d:\shoy\jobs.log"/>
  </appSettings>
  <!--
    有关 web.config 更改的说明，请参见 http://go.microsoft.com/fwlink/?LinkId=235367。

    可在 <httpRuntime> 标记上设置以下特性。
      <system.Web>
        <httpRuntime targetFramework="4.5" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.5"/>
    <httpHandlers>
      <!--/notify/*,/url.* -->
      <add verb="*" path="/router.*" type="Shoy.AjaxHelper.AjaxHandlerFactory, Shoy.AjaxHelper"/>
    </httpHandlers>
    <pages controlRenderingCompatibilityVersion="4.0"/>
  </system.web>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false"/>
    <handlers>
      <add name="ajaxHelper" verb="*" path="/router.*" type="Shoy.AjaxHelper.AjaxHandlerFactory"/>
    </handlers>
  </system.webServer>
</configuration>